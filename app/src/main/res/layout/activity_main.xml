<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.wind.annotation.MainActivity"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/am_ll_root"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >
        <TextView
            android:id="@+id/h_tv_result"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Hello World!"
            />

        <TextView
            android:id="@+id/h_tv_result2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="tv2!"
            />

        <EditText
            android:id="@+id/h_et_input1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="please input data"
            />

        <Button
            android:id="@+id/h_btn_click1"
            android:text="click here!"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <Button
            android:id="@+id/h_btn_click2"
            android:text="click here! fun2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

    </LinearLayout>


</LinearLayout>
